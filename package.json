{
  "name": "synapse-corporate-university",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:all",
    "dev:frontend": "vite",
    "dev:backend": "node scripts/bootstrap.mjs && uvicorn backend:app --reload",
    "dev:all": "concurrently \"npm:dev:backend\" \"npm:dev:frontend\"",
    "build": "vite build",
    "preview": "vite preview",
    "db:seed": "prisma db seed --schema schema.prisma",
    "db:migrate": "prisma migrate deploy --schema schema.prisma"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@prisma/client": "^5.22.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/papaparse": "^5.5.0",
    "@types/three": "^0.181.0",
    "date-fns": "^4.1.0",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.554.0",
    "papaparse": "^5.5.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.6",
    "react-select": "^5.10.2",
    "recharts": "^3.4.1",
    "remeda": "^2.32.0",
    "three": "^0.181.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "prisma": "^5.22.0",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  }
}
